 <link rel="stylesheet" href="/stylesheets/adminOrder.css" />



  <div class="container mt-5 d-flex justify-content-center">
    <div class="row">
      <div class="col-10">

         <div class="layout-wrapper layout-content-navbar">
     <div class="layout-container">
       <!-- Menu -->

       <!-- Layout container -->
       <div class="layout-page">
         <!-- Navbar -->



         <!-- / Navbar -->

         <!-- Content wrapper -->

         <div class="content-wrapper">
           <div class="container-xxl flex-grow-1 container-p-y">

             <!-- Content start -->






             <div class="card mb-4">
               <div class="card-header d-flex justify-content-between align-items-center">
                 <h5 class="mb-0">Add Product</h5>
                 <small class="text-muted float-end">Fill the Form</small>
               </div>
               <div class="card-body">
                 <form action="/admin/addProduct" method="POST" enctype="multipart/form-data">
                   <div class="mb-3">
                     <label class="form-label" for="basic-default-fullname">Product Name</label>
                     <input type="text" class="form-control" id="basic-default-fullname" name="productName" />
                   </div>
                   <div class="mb-3">
                     <label class="form-label" for="basic-default-company">Company</label>
                     <input type="text" class="form-control" id="basic-default-company" name="company" />
                   </div>


                   <div class="mb-3">
                     
                     <input type="text" class="form-control" id="basic-default-company" name="rating" value="1" hidden />
                   </div>

                   <div class="mb-3">
                     
                     <input type="text" class="form-control" id="basic-default-company" name="discount" value="1" hidden />
                   </div>
                   <div class="mb-3">

                     <div class="input-group input-group-merge">


                       <div class="mb-3"><select class="form-select" aria-label="Default select example"
                           name="category">
                           <option selected>Catogory</option>
                           {{#each categoryList}}
                           <option value="{{this.category}}">{{this.category}}</option>
                           {{/each}}

                         </select></div>







                     </div>

                   </div>
                   <div class="mb-3">
                     <label class="form-label" for="basic-default-phone">price</label>
                     <input type="number" id="basic-default-phone" class="form-control phone-mask" name="price" />
                   </div>


                   <div class="mb-3">
                     <label class="form-label" for="basic-default-phone">Avilable Quantity</label>
                     <input type="number" id="basic-default-phone" class="form-control phone-mask" name="quantity" />
                   </div>



                   <div class="mb-3">
                     <label class="form-label" for="basic-default-message">About Product</label>
                     <textarea id="basic-default-message" class="form-control" placeholder="Specifications, "
                       name="discription"></textarea>
                   </div>


                   <div class="mb-3">
                     <label class="form-label" for="basic-default-phone">Images</label>
                     <input type="file" id="basic-default-phone" class="form-control phone-mask" name="image"
                       multiple />
                   </div>





                   <button type="submit" class="btn btn-primary">Submit</button>
                 </form>
               </div>
             </div>


             <!-- Content end -->







           </div>


         </div>

         <!-- / Content -->

         <!-- Footer -->
         <footer class="content-footer footer bg-footer-theme">
