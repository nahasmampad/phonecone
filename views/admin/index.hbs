  <script type="text/javascript" src="https://unpkg.com/xlsx@0.15.1/dist/xlsx.full.min.js"></script>

  <script>
    //Table Management
    $('#tablemanager').tablemanager({
      firstSort: [
        [3, 0],
        [2, 0],
        [1, 'asc']
      ],
      disable: ["last"],
      appendFilterby: true,
      dateFormat: [
        [4, "dd-mm-yyyy"]
      ],
      debug: true,
      vocabulary: {
        voc_filter_by: 'Filtra per ',
        voc_type_here_filter: 'Search',
        voc_show_rows: 'Limit'
      },
      pagination: true,
      showrows: [10, 20, 50, 100],
      disableFilterBy: [1]
    });

    //Table Excel
    function ExportToExcel(type, fn, dl) {
      var elt = document.getElementById('tablemanager');
      var wb = XLSX.utils.table_to_book(elt, {
        sheet: "sheet1"
      });
      return dl ?
        XLSX.write(wb, {
          bookType: type,
          bookSST: true,
          type: 'base64'
        }) :
        XLSX.writeFile(wb, fn || ('MySheetName.' + (type || 'xlsx')));
    }




    $(document).ready(function () {
      $('#tbl_exporttable_to_xls').DataTable();
    });




    function deleteCoupen(coupId) {
      $.ajax({
        url: '/admin/deleteCoupen',
        data: {
          coupId: coupId
        },
        method: "post",
        success: (response) => {

          console.log("success")
          location.reload()

        }
      })
    }
  </script>





